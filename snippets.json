{
	"Create a new MFN Module": {
		"prefix": "mfn",
		"body": [
			"mfn $1 ($2) {",
			"    $3",
			"}"
		],
		"description": "Create an MFN module"
	},
    "Create a new SFN Module": {
		"prefix": "sfn",
		"body": [
			"sfn $1 ($2, s:$3) {",
			"    $4",
			"}"
		],
		"description": "Create an SFN module"
	},
	"Create a new Store Add Module": {
		"prefix": ["store_add, sadd"],
		"body": [
			"sfn $1 ($2, s:add) {",
			"    $3",
			"}"
		],
		"description": "Create a store add module"
	},
	"Create a new Store Set Module": {
		"prefix": ["store-set, sset"],
		"body": [
			"sfn $1 ($2, s:set) {",
			"    $3",
			"}"
		],
		"description": "Create a store set module"
	},
	"Create a new Store SetOnce Module": {
		"prefix": ["store-set-once, ssetOnce"],
		"body": [
			"sfn $1 ($2, s:setOnce) {",
			"    $3",
			"}"
		],
		"description": "Create a store setOnce module"
	},
	"for": {
		"prefix": ["for","for-in"],
		"body": [
			"for ${1:var} in ${2:sequence} {",
			"${3:body}",
			"}"
		],
		"description": "Inserts a for loop for iterating over a list"
	},
	"method": {
		"prefix": ["method"],
		"body": [
			"method $1($2) {",
			"$3",
			"}"
		],
		"description": "Defines a new method"
	},
	"fn": {
		"prefix": ["fn", "function"],
		"body": [
			"fn $1($2) {",
			"$3",
			"}"
		],
		"description": "Defines a new method"
	},
	"global": {
		"prefix": "global",
		"body": "global::$1",
		"description": "Inserts a global variable"
	},
	"params": {
		"prefix": ["params", "PARAMS"],
		"body": "PARAMS$1",
		"description": "Inserts the PARAMS object"
	}
}